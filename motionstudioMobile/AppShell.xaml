<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="motionstudioMobile.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:motionstudioMobile"
   xmlns:otherthing="xaml"
    Shell.FlyoutBehavior="Flyout"
    Title="motionstudioMobile">

  
    <ShellContent
     Title="Home"
     ContentTemplate="{DataTemplate local:MainPage}"
     Route="MainPage" />

    <ShellContent
    Title="Home"
    ContentTemplate="{DataTemplate local:LogInPage}"
    Route="LogIn" />

    <ShellContent
    Title="Inquiries"
    ContentTemplate="{DataTemplate local:Inquiries}"
    Route="Inquiries" />

    <ShellContent
    Title="Meals"
    ContentTemplate="{DataTemplate local:Meals}"
    Route="Meals" />



</Shell>
